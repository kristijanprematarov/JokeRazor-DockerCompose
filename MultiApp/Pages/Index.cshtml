@page
@using MultiApi.Models
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    var joke = ViewData["Joke"] as Joke;
}

<div class="text-center">
    <h1>THE JOKE</h1>
    
    <table class="table table-sm table-striped table-bordered m-2">
        <thead>
        <tr>
            <th>Name</th>
            <th>Text</th>
            <th>Category</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>@joke.Name</td>
            <td>@joke.Text</td>
            <td>@joke.Category</td>
        </tr>
        </tbody>
    </table>
</div>
